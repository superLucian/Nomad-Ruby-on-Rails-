.container-fluid, .row {
  padding: 0 !important;
  margin: 0 !important;
}

#home-section-1 .col-md-6 {
  padding: 0 0px;
}


#home-section-1 h1 {
  color: white;
  font-size: 2.4rem;
  text-align: left;
  font-weight: 700;
  text-shadow: 3px 3px 8px rgba(0, 0, 0, 0.9);
  padding-top: 60px;
}

.navbar-toggleable-md .navbar-nav .nav-link {
   font-weight: 600;
}

